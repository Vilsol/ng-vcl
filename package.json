{
  "name": "ng-vcl",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run dev:demo",
    "dev": "npm run dev:demo",
    "dev:demo": "ng serve ng-vcl-demo --plugin ~webpack.custom.js",
    "dev:ng-vcl": "ng build @vcl/ng-vcl --watch",
    "dev:wc": "ng serve ng-vcl-wc-test --plugin ~webpack.custom.js",
    "docs": "cross-env-shell npm run docs:set-env npm-run-all docs:set-env docs:clean docs:pull-current docs:build",
    "docs:set-env": "cross-env-shell GIT_BRANCH=${GIT_BRANCH:-$(git rev-parse --abbrev-ref HEAD)}",
    "docs:build": "cross-env-shell ng build --prod --outputPath=docs/${GIT_BRANCH} --plugin ~webpack.custom.js",
    "docs:deploy": "git-directory-deploy --directory docs --branch gh-pages",
    "docs:clean": "rimraf docs",
    "docs:lint": "ng lint ng-vcl-demo",
    "docs:pull-current": "git clone -b gh-pages --single-branch https://github.com/ng-vcl/ng-vcl.git docs",
    "test": "ng test @vcl/ng-vcl --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:clean": "rimraf dist/ng-vcl",
    "build:ng-vcl": "ng build @vcl/ng-vcl",
    "postbuild:ng-vcl": "cp lib/ng-vcl/*.scss dist/ng-vcl",
    "build": "npm-run-all build:clean build:ng-vcl",
    "clean": "npm-run-all build:clean docs:clean",
    "markdownlint": "markdownlint -c markdownlint.json lib/",
    "publish": "cd dist/ng-vcl && npm publish",
    "publish:next": "cd dist/ng-vcl && npm publish --tag next",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1"
  },
  "dependencies": {
    "@angular/animations": "^9.0.5",
    "@angular/cdk": "^9.1.1",
    "@angular/common": "^9.0.5",
    "@angular/compiler": "^9.0.5",
    "@angular/core": "^9.0.5",
    "@angular/elements": "^9.0.5",
    "@angular/forms": "^9.0.5",
    "@angular/platform-browser": "^9.0.5",
    "@angular/platform-browser-dynamic": "^9.0.5",
    "@angular/router": "^9.0.5",
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@mdi/font": "^4.9.95",
    "@vcl/vcl": "^1.0.0",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.4",
    "document-register-element": "1.14.3",
    "fuse.js": "^3.4.6",
    "hammerjs": "^2.0.8",
    "jsonschema": "^1.2.5",
    "ngx-build-plus": "^9.0.6",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.5",
    "@angular-devkit/build-ng-packagr": "^0.900.5",
    "@angular/cli": "^9.0.5",
    "@angular/compiler-cli": "^9.0.5",
    "@angular/language-service": "^9.0.5",
    "@types/jasmine": "^3.5.3",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^13.7.0",
    "codelyzer": "~5.2.1",
    "conventional-changelog-cli": "^2.0.31",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "cz-conventional-changelog": "^3.1.0",
    "git-directory-deploy": "^1.5.1",
    "highlight-loader": "^0.7.3",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "lerna": "^3.20.2",
    "markdown-loader": "^5.1.0",
    "ng-packagr": "^9.0.2",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.3",
    "ts-node": "^8.6.2",
    "tsickle": "0.38.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "webpack-merge": "^4.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
